@misc{cuidevicesAMT11SeriesDatasheet2019,
	title = {{AMT11} {Series} {Datasheet} - {Modular} {\textbar} {Incremental} {\textbar} {CUI} {Devices}},
	url = {https://www.cuidevices.com/product/resource/amt11.pdf},
	abstract = {CUI Devices' line of incremental encoders based on capacitive, mechanical and optical technology are available in modular and panel mount package types. Our incremental encoders feature compact footprints measuring as small as 9 mm with shaft lengths ranging from 7.5 mm to 30 mm and shaft bore diameters from 2 mm to 15.875 mm (5/8 in). Resolutions range from 10 PPR up to 4096 PPR and include 2-bit quadrature, CMOS voltage, open collector, and voltage output types. Featuring operating temperatures ranging from -40 up to +125°C as well as axial, horizontal, and radial orientations, our incremental encoders are ideal for applications including industrial automation, medical devices, test and measurement, robotics and more. Our incremental encoder portfolio is highlighted by our capacitive AMT modular encoder series, offering unmatched flexibility and durability.},
	author = {{CUI Devices}},
	month = nov,
	year = {2019}
}

@misc{cuidevicesAMT22SeriesDatasheet2019,
	title = {{AMT22} {Series} {Datasheet} - {Modular} {\textbar} {Absolute} {\textbar} {CUI} {Devices}},
	url = {https://www.cuidevices.com/product/resource/amt22.pdf},
	abstract = {CUI Devices' absolute encoders output 12 bits or 14 bits of absolute position information with Serial Peripheral Interface (SPI), RS-485, or Synchronous Serial Interface (SSI) communication. Based on proprietary capacitive technology, these absolute rotary encoders are not susceptible to environmental contaminants that typically plague encoders in industrial applications. The capacitive encoder models further offer users compact packages, current draw as low as 8 mA at 5 V, and operating temperature ranges from -40 to +125°C. CUI Devices' AMT absolute modular encoders are also available as kits that support 9 different shaft sizes from 2 mm to 8 mm with simple mounting tools designed for easy installation. Combined with radial or axial cable connections and high accuracy of ±0.2 mechanical degrees, these absolute encoders are a highly flexible and durable solution for a variety of motion control applications.},
	author = {{CUI Devices}},
	month = nov,
	year = {2019}
}

@misc{cuidevicesAMT31SeriesDatasheet2019,
	title = {{AMT31} {Series} {Datasheet} - {Commutation} {\textbar} {Rotary} {Encoders} {\textbar} {CUI} {Devices}},
	url = {https://www.cuidevices.com/product/resource/amt31.pdf},
	abstract = {CUI Devices' commutation encoders generate standard U/V/W communication signals for commutating brushless dc (BLDC) motors. Featuring 22 programmable incremental resolutions from 48 to 4096 PPR, these commutation rotary encoders are based upon proprietary capacitive technology that makes them resilient to dirt, dust, and oil that can plague traditional encoders in industrial environments. The digital nature of the capacitive encoder models also reduces the time-consuming processes of mounting and alignment to seconds with the One Touch Zero™ feature. Available as a kit, the commutation encoders support various shaft sizes from 2 mm to 5/8 in (15.875 mm) and include simple mounting tools to further simplify the assembly process. When combined with compact packages, low current draw of 16 mA at 5 V, high accuracy of ±0.2 degrees, and operating temperature ranges from -40 to +125°C, CUI Devices' commutation encoders are some of the most versatile encoders for your motion control needs.},
	author = {{CUI Devices}},
	month = nov,
	year = {2019}
}

